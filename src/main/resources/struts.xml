<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <constant name="struts.devMode" value="true" />

    <package name="clotheswishlist" extends="struts-default">
        <action name="index" class="com.example.clotheswishlist.action.ClothesAction" method="execute">
            <result name="success">/index.jsp</result>
            <result name="input">/index.jsp</result>
        </action>

        <action name="register" class="com.example.clotheswishlist.action.UserAction" method="execute">
            <result name="success">/index.jsp</result>
            <result name="input">/register.jsp</result>
            <result name="null">/register.jsp</result>
        </action>
    </package>

</struts>